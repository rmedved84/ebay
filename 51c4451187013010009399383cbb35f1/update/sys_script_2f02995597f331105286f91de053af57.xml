        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	var numberOfItems = parseInt(current.getValue('number_of_items'), 10);
	for(var i = 0; i < numberOfItems; i++){
		var itemGR = new GlideRecord('x_476139_ebay_item');
		itemGR.initialize();
		itemGR.setValue('name', current.getValue('date') + ' - ' + current.getValue('location') + ' - item ' + i);
		itemGR.setValue('purchase_date', current.date);
		itemGR.setValue('purchase_price', current.average_cog);
		itemGR.setValue('sourcing_trip', current.getUniqueValue());
		itemGR.insert();
	}

})(current, previous);]]></script>
