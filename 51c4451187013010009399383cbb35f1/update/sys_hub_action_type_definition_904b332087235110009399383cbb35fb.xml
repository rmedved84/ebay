        <value>(function execute(inputs, outputs) {

  var today = new GlideDate();
  var endDate = today.getDisplayValue() + 'T00:00:00.000Z';
  today.addDaysLocalTime(-1);
  var startDate = today.getDisplayValue() + 'T00:00:00.000Z';
  var params = {
      "limit": 1000,
      "filter": "transactionType:%7BSHIPPING_LABEL%7D",
      "filter": "transactionDate:%5B" + startDate + '..' + endDate + '%5D'
  }
  var transactions = EbayUtil.getFees(params);
  transactionList = transactions.transactions;
  for (var i = 0; i &lt; transactionList.length; i++) {
    if (transactionList[i].transactionType == 'SHIPPING_LABEL') {
      var itemGR = new GlideRecord('x_476139_ebay_item');
      if (itemGR.get('order_id', transactionList[i].orderId)) {
        itemGR.setValue('shipping', transactionList[i].amount.value);
        itemGR.update();
        continue;
      }
    }
  }

})(inputs, outputs);
</value>
